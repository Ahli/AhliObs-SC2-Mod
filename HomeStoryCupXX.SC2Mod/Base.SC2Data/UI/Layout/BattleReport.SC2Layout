            </Frame>
        </Frame>

        <Frame type="Frame" name="DmgValue">
            <Anchor side="Top" relative="$parent/$parent/SubtitlesFrame/DmgLabel" pos="Max" offset="5"/>
            <Anchor side="Left" relative="$parent/ResourcesLostFrame/ResourcesLostGas" pos="Min" offset="0"/>
            <Width val="150"/>        
            <Height val="30"/>

            <Frame type="PlayerScoreValueFrame" name="FormattedScoreText">
                <Anchor relative="$parent"/>

                <Frame type="ScoreValueLabelFrame" name="ScoreValueFrame0">
                    <Anchor relative="$parent"/>
                    <ScoreValueLink val="BattleReportDamageDealt"/>
                    
                    <Frame type="Label" name="Label">
                        <Anchor relative="$parent"/>
                        <IgnoreParentColor val="true"/>
                        <Alpha val="0"/>
                    </Frame>
                </Frame>
            </Frame>

            <Frame type="DataRefFrame" name="Value_Percent">
                <Argument name="Score" val="BattleReportDamageDealtPercent"/>
            </Frame>

            <Frame type="TextFormatFrame" name="FormattedText">
                <Anchor relative="$parent"/>
                <FormatText val="%0% (%1%%)"/>
                <Replacement index="0" val="{$parent/FormattedScoreText/ScoreValueFrame0/Label/@Text}"/>
                <Replacement index="1" val="{$parent/Value_Percent/@StringValue}"/>
            </Frame>

            <Frame type="Label" name="Label">
                <Anchor relative="$parent"/>
                <Text val="{$parent/FormattedText/@Text}"/>
                <Style val="BattleReportStatL"/>
                <Options val="NoWrapping|ScaleToPreventTruncation"/>
            </Frame>
        </Frame>
    </Frame>

