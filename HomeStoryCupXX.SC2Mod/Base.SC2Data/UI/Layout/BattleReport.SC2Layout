                <Anchor side="Top" relative="$parent/PlayerNameL" pos="Max" offset="0"/>
                <Anchor side="Bottom" relative="AbilityButtons" pos="Max" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-15"/>
            </Frame>

            <Frame type="BlurFrame" name="GraphsFrame">
                <Anchor relative="$parent"/>
                <BlurAmount val="0.5"/>
                <RenderType val="LDR"/>
                <Alpha val="224"/>

                <Frame type="Frame" name="DamageDealtBarGraph" template="BattleReport/ComparisonBarTemplate">
                    <Anchor side="Top" relative="$parent/$parent/SubtitlesFrame/DmgLabel" pos="Max" offset="5"/>
                    <Anchor side="Left" relative="$parent/$parent/StatsL/DmgValue" pos="Max" offset="10"/>
                    <Anchor side="Right" relative="$parent/$parent/StatsR/DmgValue" pos="Min" offset="-15"/>
                    <Height val="30"/>

                    <Frame type="DataRefFrame" name="ValueL">
                        <Argument name="Score" val="BattleReportDamageDealtPercent"/>
                        <PlayerId val="{$BattleReport/PlayerL/@PlayerId}"/>
                    </Frame>
                    <Frame type="DataRefFrame" name="ValueTotal">
                        <Argument name="Score" val="BattleReportDamageDealtTotal"/>
                        <PlayerId val="{$BattleReport/PlayerL/@PlayerId}"/>
                    </Frame>

                    <Frame type="ProgressBar" name="ProgressBarL">
                        <Frame type="Frame" name="FillImageContainer">
                            <Frame type="Image" name="FillImage">
                                <DesaturationColor val="{$BattleReport/PlayerL/@Color}"/>
                            </Frame>
                        </Frame>
                    </Frame>

                    <Frame type="Image" name="FillImageR">
                        <DesaturationColor val="{$BattleReport/PlayerR/@Color}"/>
                    </Frame>
                </Frame>

                <Frame type="Frame" name="MineralsLostBarGraph" template="BattleReport/ComparisonBarTemplate">
                    <Anchor side="Top" relative="$parent/$parent/SubtitlesFrame/ResourcesLostLabel" pos="Max" offset="5"/>
                    <Anchor side="Left" relative="$parent/DamageDealtBarGraph" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent/DamageDealtBarGraph" pos="Max" offset="0"/>
                    <Height val="30"/>

                    <Frame type="DataRefFrame" name="ValueL">
                        <Argument name="Score" val="BattleReportMineralsDestroyedPercent"/>
                        <PlayerId val="{$BattleReport/PlayerL/@PlayerId}"/>
                    </Frame>
                    <Frame type="DataRefFrame" name="ValueTotal">
                        <Argument name="Score" val="BattleReportMineralsDestroyedTotal"/>
                        <PlayerId val="{$BattleReport/PlayerL/@PlayerId}"/>
                    </Frame>

                    <Frame type="ProgressBar" name="ProgressBarL">
                        <Frame type="Frame" name="FillImageContainer">
                            <Frame type="Image" name="FillImage">
                                <DesaturationColor val="{$BattleReport/PlayerL/@Color}"/>
                            </Frame>
                        </Frame>
                    </Frame>

                    <Frame type="Image" name="FillImageR">
                        <DesaturationColor val="{$BattleReport/PlayerR/@Color}"/>
                    </Frame>
                </Frame>

                <Frame type="Frame" name="GasLostBarGraph" template="BattleReport/ComparisonBarTemplate">
                    <Anchor side="Top" relative="$parent/MineralsLostBarGraph" pos="Max" offset="15"/>
                    <Anchor side="Left" relative="$parent/DamageDealtBarGraph" pos="Min" offset="0"/>
                    <Anchor side="Right" relative="$parent/DamageDealtBarGraph" pos="Max" offset="0"/>
                    <Height val="30"/>

                    <Frame type="DataRefFrame" name="ValueL">
                        <Argument name="Score" val="BattleReportVespeneDestroyedPercent"/>
                        <PlayerId val="{$BattleReport/PlayerL/@PlayerId}"/>
                    </Frame>
                    <Frame type="DataRefFrame" name="ValueTotal">
                        <Argument name="Score" val="BattleReportVespeneDestroyedTotal"/>
                        <PlayerId val="{$BattleReport/PlayerL/@PlayerId}"/>
                    </Frame>

                    <Frame type="ProgressBar" name="ProgressBarL">
                        <Frame type="Frame" name="FillImageContainer">
                            <Frame type="Image" name="FillImage">
                                <DesaturationColor val="{$BattleReport/PlayerL/@Color}"/>
                            </Frame>
                        </Frame>
                    </Frame>

                    <Frame type="Image" name="FillImageR">
                        <DesaturationColor val="{$BattleReport/PlayerR/@Color}"/>
                    </Frame>
                </Frame>
            </Frame>

            <Frame type="Frame" name="SubtitlesFrame">
                <Anchor relative="$parent"/>
                <RenderPriority val="515"/>

                <Frame type="Label" name="UnitsKilledLabel">
                    <Anchor side="Top" relative="$parent/$parent/PlayerNameL" pos="Max" offset="15"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Width val="250"/>
                    <Height val="30"/>
                    <Text val="Units Killed"/>
                    <Style val="BattleReportSectionTitle"/>
                </Frame>

                <Frame type="Label" name="ResourcesLostLabel">
                    <Anchor side="Top" relative="$parent/$parent/ButtonsL/UnitButtons" pos="Max" offset="15"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Width val="250"/>
                    <Height val="30"/>
                    <Style val="BattleReportSectionTitle"/>
                    <Text val="Resources Destroyed"/>
                </Frame>

                <Frame type="Label" name="DmgLabel">
                    <Anchor side="Top" relative="$parent/$parent/StatsL/ResourcesLostFrame/ResourcesLostGas" pos="Max" offset="15"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Width val="250"/>
                    <Height val="30"/>
                    <Style val="BattleReportSectionTitle"/>
                    <Text val="Damage Dealt"/>
                </Frame>

                <Frame type="Label" name="AbilitiesUsedLabel">
                    <Anchor side="Top" relative="$parent/DmgLabel" pos="Max" offset="55"/>
                    <Anchor side="Left" relative="$parent" pos="Mid" offset="0"/>
                    <Anchor side="Right" relative="$parent" pos="Mid" offset="0"/>
                    <Width val="250"/>
                    <Height val="30"/>
                    <Text val="Abilities Used"/>
                    <Style val="BattleReportSectionTitle"/>
                </Frame>
            </Frame>
        </Frame>
        <Frame type="ToggleControl" name="BattleReportToggle">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Height val="0"/>
            <Width val="0"/>
            <Shortcut val="Shift+B"/>
            <ShortcutEnabledForParticipants val="false"/>
            <ShortcutEnabledForObservers val="true"/>
            <Visible val="false"/>

            <Animation name="Show">
                <Event event="OnShown" action="Reset,Play" />
                <Controller type="Fade" end="Pause" frame="$BattleReport">
                    <Key type="Curve" time="0" value="0" />
                    <Key type="Curve" time="0.125" value="255"/>
                </Controller>
                <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$BattleReport">
                    <Key type="Curve" time="0" value="30" inout="Linear" />
                    <Key type="Curve" time="0.125" value="0" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="$BattleReport">
                    <Key type="Bool" time="0" value="true"/>
                </Controller>
            </Animation>

            <Animation name="Hide">
                <Event event="OnHidden" action="Reset,Play"/>
                <Controller type="Fade" end="Pause" frame="$BattleReport">
                    <Key type="Curve" time="0" value="255"/>
                    <Key type="Curve" time="0.125" value="0"/>
                </Controller>
                <Controller type="Anchor" end="Pause" side="Top" relative="true" frame="$BattleReport">
                    <Key type="Curve" time="0" value="0" inout="Linear"/>
                    <Key type="Curve" time="0.125" value="30" inout="Slow,Linear"/>
                </Controller>
                <Controller type="Visibility" end="Pause" frame="$BattleReport">
                    <Key type="Bool" time="0" value="True"/>
                    <Key type="Bool" time="0.125" value="False"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>
</Desc>
