    <Frame type="Frame" name="ComparisonBarTemplate">
        <Frame type="DataRefFrame" name="ValueL">
        </Frame>
        <Frame type="DataRefFrame" name="ValueTotal">
        </Frame>

        <Frame type="ProgressBar" name="ProgressBarL">
            <Anchor relative="$parent"/>
            <AcceptsMouse val="false"/>
            <UseFillContainer val="true"/>
            <Vertical val="False"/>
            <MinValue val="0"/>
            <!--<MaxValue val="{$parent/ValueTotal/@RealValue}"/>
                   This has issues when MaxValue was 0
                      => first update causes progressbar's value to always be 0
                         => calc percentage in triggers instead -->
            <MaxValue val="100"/>
            <Value val="{$parent/ValueL/@RealValue}"/>

            <Frame type="Frame" name="FillImageContainer">
                <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>

                <Frame type="Image" name="FillImage">
                    <Anchor relative="$parent/$parent"/>
                    <Texture val="Assets\Textures\ui_mission_leaderboard_healthbar_red.dds"/>
                    <TextureType val="Border"/>
                    <Desaturated val="True"/>
                </Frame>
            </Frame>
        </Frame>

        <Frame type="Image" name="FillImageR">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent/ProgressBarL/FillImageContainer" pos="Max" offset="0"/>
            <Texture val="Assets\Textures\ui_mission_leaderboard_healthbar_red.dds"/>
            <TextureType val="Border"/>
            <Desaturated val="True"/>
        </Frame>

        <StateGroup name="ValueTotalState">
            <State name="0">
                <When type="Property" frame="ValueTotal" RealValue="0"/>
                <Action type="SetProperty" frame="ProgressBarL/FillImageContainer/FillImage" visible="false"/>
                <Action type="SetProperty" frame="FillImageR" visible="false"/>
            </State>
        </StateGroup>
    </Frame>

