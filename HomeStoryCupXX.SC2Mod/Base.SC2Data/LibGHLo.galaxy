//--------------------------------------------------------------------------------------------------
// Trigger: Battle Report - Unit Is Damaged
//--------------------------------------------------------------------------------------------------
bool libGHLo_gt_BattleReportUnitIsDamaged_Func (bool testConds, bool runActions) {
    // Variable Declarations
    int lv_victimPlayer;
    int lv_opposingSideIndex;

    // Automatic Variable Declarations
    // Variable Initialization
    lv_victimPlayer = EventPlayer();

    // Actions
    if (!runActions) {
        return true;
    }

    if ((lv_victimPlayer != EventUnitDamageSourcePlayer()) && (EventUnitDamageAttempted() > 0.0)) {
        lv_opposingSideIndex = libGHLo_gf_BattleReportGetOpposingPlayerSideIndex(lv_victimPlayer);
        if ((lv_opposingSideIndex != -1)) {
            if ((!(libGHLo_gv_battleReport.lv_battleActive))) {
                libGHLo_gf_BattleReportStartBattle();
            }
            else {
                libGHLo_gv_battleReport.lv_resetBattleEndingTimer = true;
            }
            libGHLo_gv_battleReport.lv_updateDueToDamage = true;
            libGHLo_gv_battleReportSideData[lv_opposingSideIndex].lv_damageDealt += EventUnitDamageAmount();
        }
        else {
            return false;
        }
    }
    else {
        return false;
    }
    return true;
}

