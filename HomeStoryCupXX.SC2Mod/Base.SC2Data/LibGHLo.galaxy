//--------------------------------------------------------------------------------------------------
// Trigger: Battle Report - Unit Dies
//--------------------------------------------------------------------------------------------------
bool libGHLo_gt_BattleReportUnitDies_Func (bool testConds, bool runActions) {
    // Variable Declarations
    string lv_unitType;
    string lv_unitTypeForCounter;
    string lv_unitTypeLeaderAlias;
    unit lv_unit;
    int lv_opposingSideIndex;
    int lv_mineralsCost;
    int lv_vespeneCost;
    int lv_owner;

    // Automatic Variable Declarations
    // Variable Initialization

    // Actions
    if (!runActions) {
        return true;
    }

    if (libGHLo_gv_battleReport.lv_battleActive) {
        lv_unit = EventUnit();
        if ((UnitHasBehavior2(lv_unit, "NeuralParasite") == true)) {
            lv_owner = UnitGetOwner(lv_unit);
            lv_opposingSideIndex = libGHLo_gf_BattleReportGetPlayerSideIndex(lv_owner);
            lv_owner = libGHLo_gv_battleReport.lv_playerIdSide[libGHLo_gf_BattleReportGetOpposingPlayerSideIndex(lv_owner)];
        }
        else {
            lv_owner = UnitGetOwner(lv_unit);
            lv_opposingSideIndex = libGHLo_gf_BattleReportGetOpposingPlayerSideIndex(lv_owner);
        }
        if ((lv_opposingSideIndex != -1)) {
            lv_unitType = UnitGetType(lv_unit);
            if ((UnitHasBehavior2(lv_unit, "Hallucination") == false) && (lv_unitType != "MULE") && (EventUnitDamageDeathCheck(c_deathTypeRemove) == false)) {
                if (libGHLo_gf_BattleReportIsTrackedUnitType(lv_unit, lv_unitType)) {
                    lv_unitTypeLeaderAlias = (CatalogFieldValueGet(c_gameCatalogUnit, lv_unitType, "LeaderAlias", lv_owner));
                    if ((lv_unitTypeLeaderAlias != null) && (lv_unitTypeLeaderAlias != lv_unitType)) {
                        lv_unitType = lv_unitTypeLeaderAlias;
                    }

                    lv_unitTypeForCounter = lv_unitType;
                    if ((lv_unitType == "BanelingCocoon")) {
                        lv_unitTypeForCounter = "Zergling";
                    }
                    else {
                        if ((lv_unitType == "RavagerCocoon")) {
                            lv_unitTypeForCounter = "Roach";
                        }
                        else {
                            if ((lv_unitType == "BroodLordCocoon")) {
                                lv_unitTypeForCounter = "Corruptor";
                            }
                            else {
                                if ((lv_unitType == "LurkerMPEgg")) {
                                    lv_unitTypeForCounter = "Hydralisk";
                                }
                                else {
                                    if ((lv_unitType == "TransportOverlordCocoon")) {
                                        if ((UnitHasBehavior2(lv_unit, "AHLI_fixUnitLostResourcesOfOverlordTransport") == true)) {
                                            lv_unitTypeForCounter = "OverlordTransport";
                                        }
                                        else {
                                            lv_unitTypeForCounter = "Overlord";
                                        }
                                    }

                                }
                            }
                        }
                    }
                    libGHLo_gf_BattleReportIncrementUnitTypeKill(lv_opposingSideIndex, lv_unitTypeForCounter);
                }

                if ((lv_unitType != "AutoTurret")) {
                    lv_mineralsCost = libGHLo_gf_GetFixedUnitResourceCost(lv_unit, c_unitCostMinerals, lv_owner);
                    lv_vespeneCost = libGHLo_gf_GetFixedUnitResourceCost(lv_unit, c_unitCostVespene, lv_owner);
                    if (((EventUnitDamageDeathCheck(c_deathTypeCancel) == true) || (EventUnitDamageDeathCheck(c_deathTypeSalvage) == true))) {
                        if ((UnitTypeTestAttribute(lv_unitType, c_unitAttributeStructure) == true) && (UnitTypeTestAttribute(lv_unitType, c_unitAttributeBiological) == true)) {
                            lv_mineralsCost = MaxI((lv_mineralsCost - CatalogFieldValueGetAsInt(c_gameCatalogUnit, "Drone", "CostResource[" + IntToString(0) + "]", lv_owner)), 0);
                            lv_vespeneCost = MaxI((lv_vespeneCost - CatalogFieldValueGetAsInt(c_gameCatalogUnit, "Drone", "CostResource[" + IntToString(1) + "]", lv_owner)), 0);
                        }

                        lv_mineralsCost /= 4;
                        lv_vespeneCost /= 4;
                    }

                    if (((lv_mineralsCost > 0) || (lv_vespeneCost > 0))) {
                        libGHLo_gf_BattleReportIncrementResourceDamage(lv_opposingSideIndex, lv_mineralsCost, lv_vespeneCost);
                    }

                }

                return true;
            }

        }

    }

    return false;
    return true;
}

